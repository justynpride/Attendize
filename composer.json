{
    "name": "attendize/attendize",
    "version": "2.8.0",
    "description": "A free and open-source event management and ticket selling application.",
    "keywords": [
        "event management",
        "ticket selling",
        "tickets",
        "events"
    ],
    "license": "AAL",
    "type": "project",
    "homepage": "https://www.attendize.com",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/dbal": "^3.1",
        "dompdf/dompdf": "^2.0",
        "graham-campbell/markdown": "^15.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laracasts/utilities": "^3.2",
        "laravel/framework": "^10.10",
        "laravel/socialite": "^5.8",
        "laravel/tinker": "^2.8",
        "laravelcollective/html": "^6.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/omnipay": "^3.2",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.8",
        "mews/purifier": "^3.4",
        "milon/barcode": "^10.0",
        "marvinlabs/laravel-discord-logger": "^1.4",
        "nitmedia/wkhtml2pdf": "^2.1",
        "omnipay/common": "^3.2",
        "omnipay/dummy": "^3.0",
        "omnipay/paypal": "^3.0",
        "omnipay/stripe": "^3.2",
        "php-http/curl-client": "^2.3",
        "php-http/message": "^1.16",
        "predis/predis": "^2.2",
        "stripe/stripe-php": "^12.1",
        "superbalist/php-money": "^2.0",
        "vinelab/http": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13"
    },
    "autoload": {
        "classmap": [
            "app/Http/Controllers",
            "app/Models",
            "app/Attendize",
            "app/Services",
            "app/Services/PaymentGateway",
            "database",
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Attendize\\": "app/Models"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"        
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
         "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "potsky/laravel-localization-helpers"
            ]
        }
    },
    "minimum-stability": "stable"
}
